<!DOCTYPE html>
<html lang="en">
<head>
 <% include partials/head %> 
</head>
	<body id="myPage" data-spy="scroll" data-target=".navbar" data-offset="100" class="home-page">
		<!--header -->
		<% include partials/header %> 
		<!--//-->      
		<section class="dashboard-section">	
			<div class="wrapper">
				<div class="row row-offcanvas row-offcanvas-left">
					<!-- sidebar -->
					<div class="column col-sm-3 col-xs-1 sidebar-offcanvas" id="sidebar">
						<% include partials/sidebar %>								
					</div>
					<!-- /sidebar -->
					<!-- main right col -->
					<div class="column col-sm-9 col-xs-11 main-dashcontent" id="main">    <h2 class="pro-heading"><%= page %></h2>
						
						
						<!-- userlist-->
						<div class="row clearfix mt-20" id="Evt-calc">
							<div class="col-md-12">
								<h2>Displaying Support Tickets</h2>
								<div class="table-responsive mt-20">
									<table class="table table-bordered users-list">
										<thead>
											<tr>
												<th align="center">TICKET ID</th>
												<th align="center">SUBJECT</th>
												<th align="center">Message</th>
												<th align="center">DETAILS</th>
											</tr>
										</thead>
										<tbody>
											<%
												if(support_list != '')
												{
													for(var i=0; i < support_list.length; i++)
													{
														%>
															<tr>
																<td align="center">00<%= support_list[i].id %></td>
																<td align="center"><%= support_list[i].subject %></td>
																<td align="center"><%= support_list[i].message %></td>
																<td align="center"><a href="/ticketDetails?t=<%= support_list[i].id %>">DETAILS</a></td>
															</tr>														
														<%
													}
												}
												else
												{
													%>
														<tr>
															<td colspan="4">No record found</td>
														</tr>														
													<%
												}
											%>
										
										</tbody>
										
									</table>
								</div>

							</div>
						</div>
						<!--//-->
					</div>
				</div>
			</div>
		</section>
	<% include partials/footer %>
	<% include partials/script %>	
	<script>
		preloader();
	</script>
</body>
</html>