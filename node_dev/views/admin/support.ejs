<!DOCTYPE html>
<html>
  <head>
    <% include partials/head %> 
  </head>
  <body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
      <header class="main-header">
		<% include partials/header %>         
      </header>
      <!-- Left side column. contains the logo and sidebar -->
	  <% include partials/sidebar %>       
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
		<section class="content-header">
			<h1>
				<%= page %>
				<small>Control panel</small>
			</h1>
			<ol class="breadcrumb">
				<li>
					<a href="/admin"><i class="fa fa-dashboard"></i>Dashboard</a>
				</li>
				<li class="active"><%= page %></li>
			</ol>
		</section>
        <!-- Main content -->
        <section class="content">                
			<div id="content">
				<div class="row">				
					<div class="col-md-12 column">				
						<div class="box box-primary">
							<div class="box-header">
								<h3 class="box-title"><%= page %> details</h3> 
							</div>							
							<div class="box-body">
								<table id="example1" class="table table-bordered table-striped">
									<thead>
										<tr>
											<th>Username</th>  
											<th>Email</th>  
											<th>Subject</th>  
											<th>Message</th>  
											<th>Ticket</th>  
											<th>Action</th>  
										</tr>  			
									</thead>
									<tbody>	
										<% 
											if(support_record)
											{
												for(var i=0; i < support_record.length; i++)
												{ 
													%>
														<tr>
															<td><%= support_record[i].name %></td>
															<td><%= support_record[i].email %></td>
															<td><%= support_record[i].subject %></td>
															<td><%= support_record[i].message %></td>
															<td><a href="/admin/supportChat?e=<%= support_record[i].id %>">00<%= support_record[i].id %></a></td>
															<td>			
																<a class="confirm" onclick="return delete_detail('removeSupportDetail?e=<%= support_record[i].sId %>');" href="javascript:void(0);" title="Remove">
																	<i class="fa fa-trash-o fa-2x text-danger"></i>
																</a>	
															</td>
														</tr>
													<% 
												}
											}
											else
											{
												%>
													<tr>
														<td colspan="4">No Records Found</td>
													</tr>	
												<%
											}
										%>
									</tbody>
								</table>
							</div><!-- /.box-body -->
							<!-- /.box -->
						</div>
					</div>
				</div>
			</div>
		</section>
      </div><!-- /.content-wrapper -->
      <% include partials/footer %> 
      <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->
    <% include partials/script %> 	
  </body>
</html>
